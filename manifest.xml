    <Host Name="Workbook" />
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://USERNAME.github.io/pmfusion-addin/taskpane.html" />
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
